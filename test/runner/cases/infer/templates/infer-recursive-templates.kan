// (i8, i32, i8) -> *i8
// (*i8, i8, i32) -> *i8
// (i32, *i8, i8) -> i32
// (i32, i8, *i8) -> i32
// (i8, *i8, i32) -> i8
// (i8, i32, *i8) -> i8
def [A, B, C] f(a: A, b: B, c: C): A {
    f(a, b, c);
    f(a, c, b);
    f(b, a, c);
    f(b, c, a);
    f(c, a, b);
    f(c, b, a);
    return a;
}

// (*i8, i32, i8) -> *i8
def [A, B, C] f2(a: A, b: B, c: C): A {
    f(a, b, c);
    f(a, c, b);
    f(b, a, c);
    f(b, c, a);
    f(c, a, b);
    f(c, b, a);
    return a;
}

def main() {
    let i = 1;
    let j = f2("", i, ' ');
    // these should not create another instance
    f2("", 0, ' ');
    let k = i;
    f2("", k, ' ');
}

