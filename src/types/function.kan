import ":std/vec";
import ":source/ident";

import "scope";
import "ctx" as _;
import "types" as _;
import "instances" as _;

type ABI enum {
    C, Kantan
}

type Function struct {
    c_variadic: bool,
    abi: ABI,
    name: ident.Ident, // will be empty for function pointers
    instances: *InstanceSet,
    num_params: usize,
    params_head: *TypeNode,
    ret: *Type
}

def (f: *Function) is_known_at_compile_time(): bool {
    return !f.name.is_empty();
}
